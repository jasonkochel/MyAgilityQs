{
  "name": "@my-agility-qs/server",
  "version": "1.0.0",
  "description": "AWS Lambda API server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "node build.mjs",
    "build:watch": "node build.mjs --watch",
    "dev": "sam local start-api --port 3001 --host 0.0.0.0",
    "dev:watch": "concurrently \"npm run build:watch\" \"sam local start-api --port 3001 --host 0.0.0.0 --watch\"",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "npm run build && node -e \"import('./dist/index.js').then(({handler}) => console.log('✅ Build and imports working!')).catch(e => {console.error('❌', e.message); process.exit(1);})\""
  },
  "dependencies": {
    "@middy/core": "^6.3.2",
    "@middy/http-cors": "^6.3.2",
    "@middy/http-error-handler": "^6.3.2",
    "@middy/http-json-body-parser": "^6.3.2",
    "@middy/http-router": "^6.3.2",
    "@middy/validator": "^6.3.2",
    "@my-agility-qs/shared": "*",
    "aws-sdk": "^2.1691.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.140",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
